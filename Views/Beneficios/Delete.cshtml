@model appbeneficiencia.Models.Beneficio
<!--Vista eliminar beneficios-->
@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar Beneficios</h1>

<h3>¿Desea Eliminar los beneficios?</h3>
<div>
    <h4>Beneficio</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DetalleBeneficio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DetalleBeneficio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Soporte)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Soporte)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdPatrocinadorNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdPatrocinadorNavigation.IdPatrocinador)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdBeneficio" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-secondary">Regresar</a>
    </form>
</div>
