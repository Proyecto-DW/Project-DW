    <form asp-action="Delete">
        <input type="hidden" asp-for="IdBeneficiario" />
        <br />
        <input type="submit" value="Eliminar" class="btn btn-outline-danger" />
        <a asp-action="Index" class="btn btn-outline-info">Regresar</a>
    </form>
</div>
