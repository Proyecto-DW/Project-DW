@model appbeneficiencia.Models.Modulo

@{
    ViewData["Title"] = "Eliminar Módulo";
}

<div>
    <h4>Módulo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Modulo1)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Modulo1)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaCreacion)
        </dt>
